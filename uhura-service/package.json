{
  "$schema": "https://json.schemastore.org/package.json",
  "name": "@rogwilco/uhura-service",
  "version": "0.0.0",
  "private": true,
  "license": "UNLICENSED",
  "author": "Nick Williams",
  "main": "src/main",
  "scripts": {
    "build": "nest build -p tsconfig.build.json",
    "ci:build": "npx yarn install --frozen-lockfile",
    "ci:quality": "yarn lint . && yarn prettier . --check",
    "ci:test": "yarn test:cov --ci --colors",
    "ci:verify": "yarn ci:quality && yarn ci:test",
    "predist": "yarn version --no-git-tag-version --no-commit-hooks --new-version ${npm_package_version}${BUILD_METADATA:++$BUILD_METADATA}",
    "dist": "nest build -p tsconfig.dist.json",
    "postdist": "npx yarn install --frozen-lockfile --production --modules-folder out/dist/node_modules",
    "format": "f() { yarn lint --fix ${@}; yarn prettier --write ${@:-.}; }; f",
    "lint": "eslint",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node src/main",
    "start:watch": "nest start --watch",
    "test": "jest --runInBand",
    "test:cov": "yarn test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "yarn test test/",
    "test:unit": "yarn test src/",
    "test:watch": "yarn test --watch"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.8",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.3.8",
    "@nestjs/devtools-integration": "^0.1.6",
    "@nestjs/platform-express": "^10.3.8",
    "@nestjs/swagger": "^7.3.1",
    "@nestjs/terminus": "^10.2.3",
    "@nestjs/typeorm": "^10.0.2",
    "axios": "^1.6.8",
    "pg": "^8.11.5",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.3.2",
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.8",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/jest-json-schema": "^6.1.4",
    "@types/node": "^20.12.11",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "jest-json-schema": "^6.1.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "webpack": "^5.91.0"
  },
  "engines": {
    "node": "20.x",
    "npm": "10.x"
  }
}
